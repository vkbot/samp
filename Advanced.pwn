stock Encrypt(string[])
{
printr("Encrypt");
	for(new s=0; s < strlen(string); s++)
	  {
		  string[s] += (3^s) * (s % 15);
		  if(string[s] > (0xff))
		  {
			  string[s] -= 256;
		  }
	  }
	return 1;
}

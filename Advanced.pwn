	{
		SendClientMessage(playerid, COLOR_WHITE, "Необходимо авторизоваться!");
		Kick(playerid);
		return 1;
	}
	if(PlayerInfo[playerid][pMember] > 0)
	{
		SetPlayerSkin(playerid, PlayerInfo[playerid][pModel]);

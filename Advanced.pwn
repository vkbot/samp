public WantedPlayer(playerid)
{
    if(Pursiut[playerid] != -1)
    {
        new Float:pos[3];
        GetPlayerPos(Pursiut[playerid],pos[0],pos[1],pos[2]);
        if(GetDistanceBetweenPlayers(playerid,Pursiut[playerid]) > 250)
        {
            Pursiut[playerid] = -1;
            CheckWant[playerid][1] = 0;
            SendClientMessage(playerid,-1,"преступник скрылся");
            KillTimer(TimerWant[playerid]);
            DisablePlayerCheckpoint(playerid);
            return 1;
        }
        SetPlayerCheckpoint(playerid, pos[0],pos[1],pos[2], 4.0);
    }
    return 1;
}
public OnPlayerEditObject(playerid, playerobject, objectid, response, Float:fX, Float:fY, Float:fZ, Float:fRotX, Float:fRotY, Float:fRotZ)
{
	if(GetPVarInt(playerid, "addatm") !=1)
	{
		if(GetPVarInt(playerid, "ModelID") !=0)
		{
			if(response == 1)
			{
				SPD(playerid, DIALOG_SAVE_1, DIALOG_STYLE_MSGBOX, "Объект изменён",\
				"Желаете ли вы сохранить объект в файл?\n\nФайл с объектами находится в {FFFFFF}scriptfiles/soe/*.txt",\
				"Сохранить...","Отмена");
				O[foX] = fX;
				O[foY] = fY;
				O[foZ] = fZ;
				O[roX] = fRotX;
				O[roY] = fRotY;
				O[roZ] = fRotZ;
			}
		}
	}
	else
	{
		if(response == 1)
		{
			ATMInfo[TOTALATM][aid]= TOTALATM;
			ATMInfo[TOTALATM][aX]= fX;
			ATMInfo[TOTALATM][aY]= fY;
			ATMInfo[TOTALATM][aZ]= fZ;
			ATMInfo[TOTALATM][arZ]= fRotZ;
			LABELATM[TOTALATM] = Create3DTextLabel("MTC ATM\nНамжите ENTER\nИли введите /atm",COLOR_GREEN, fX,fY,fZ+1,20.0,0,1);
			saveatm();
		}
	}
}
stock saveatm()
{
	new idx;
	new File: file2;
	while (idx < sizeof(ATMInfo))
	{
		if(ATMInfo[idx][aid] !=0)
		{
			new coordsstring[256];
			format(coordsstring, sizeof(coordsstring), "%d|%f|%f|%f|%f\n",
			ATMInfo[idx][aid],
			ATMInfo[idx][aX],
			ATMInfo[idx][aY],
			ATMInfo[idx][aZ],
			ATMInfo[idx][arZ]);
			if(idx == 0)
			{
				file2 = fopen("logs/atm.ini", io_write);
			}
			else
			{
				file2 = fopen("logs/atm.ini", io_append);
			}
			fwrite(file2, coordsstring);
			fclose(file2);
		}
		idx++;
	}
}
stock LoadATM()
{
	new arrCoords[5][64];
	new strFromFile2[256];
	new File: file = fopen("logs/atm.ini", io_read);
	new idx;
	if(file)
	{
		while(idx < sizeof(ATMInfo))
		{
			fread(file, strFromFile2);
			split(strFromFile2, arrCoords, '|');
			if(strval(arrCoords[0]) != 0)
			{
				ATMInfo[idx][aid]= strval(arrCoords[0]);
				ATMInfo[idx][aX]= floatstr(arrCoords[1]);
				ATMInfo[idx][aY]= floatstr(arrCoords[2]);
				ATMInfo[idx][aZ]= floatstr(arrCoords[3]);
				ATMInfo[idx][arZ]= floatstr(arrCoords[4]);
				TOTALATM++;
				ATM[idx] = CreateObject(2754, ATMInfo[idx][aX], ATMInfo[idx][aY], ATMInfo[idx][aZ], 0.0, 0.0, ATMInfo[idx][arZ]);
				LABELATM[idx] = Create3DTextLabel("Нажмите 'ENTER'",COLOR_GREEN, ATMInfo[idx][aX],ATMInfo[idx][aY],ATMInfo[idx][aZ]+1,20.0,0,1);
			}
			idx++;
		}
	}
	printf("%d банкоматов было успешно загружено!", TOTALATM);
	fclose(file);
	return 1;
}
